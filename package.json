{
  "name": "webdriverjs-example",
  "version": "1.0.0",
  "description": "Example tests using webdriverjs",
  "engines": {
    "node": ">=14"
  },
  "standard": {
    "globals": [
      "expect",
      "beforeAll",
      "afterAll",
      "describe",
      "test",
      "beforeEach",
      "afterEach"
    ]
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)"
    ],
    "testTimeout": 90000,
    "verbose": true
  },
  "scripts": {
    "prejest": "jest --clearCache",
    "jest": "jest",
    "test": "npm run jest",
    "watch": "npm run jest --watchAll",
    "watch:api": "npm run jest tests/apiTest.js --watch",
    "pretty": "standard --fix"
  },
  "author": "brine",
  "license": "MIT",
  "dependencies": {
    "babel-jest": "^26.6.3",
    "chromedriver": "^94.0.0",
    "jest": "^27.2.4",
    "selenium-webdriver": "^4.0.0-rc-2",
    "standard": "^16.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6"
  }
}
